{
  "name": "real-estate-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:LoginFolder": "cypress run --spec cypress/e2e/ui/login/*.cy.js --browser chrome",
    "test:login-positive": "cypress run --spec cypress/e2e/ui/login/*.cy.js --browser chrome",
    "test:login-negative": "cypress run --spec cypress/e2e/ui/login/*.cy.js --browser chrome",
    "test:logout-positive": "cypress run --spec cypress/e2e/ui/login/*.cy.js --browser chrome",
    "test:RegisterFolder": "cypress run --spec cypress/e2e/ui/register/*.cy.js --browser chrome",
    "test:register-user-positive": "cypress run --spec cypress/e2e/ui/register/*.cy.js --browser chrome",
    "test:register-user-negative": "cypress run --spec cypress/e2e/ui/register/*.cy.js --browser chrome",
    "test:register-realtor-positive": "cypress run --spec cypress/e2e/ui/register/*.cy.js --browser chrome",
    "test:register-realtor-negative": "cypress run --spec cypress/e2e/ui/register/*.cy.js --browser chrome",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:parallel:register": "cypress-parallel -s cy:run -t 4 -d 'cypress/e2e/ui/register/**/*.cy.js' -e '**/*.DS_Store'",
    "cy:parallel:login": "cypress-parallel -s cy:run -t 3 -d 'cypress/e2e/login/**/*.cy.js' -e '**/*.DS_Store'",
    "clean:reports": "rm -rf cypress/reports",
    "pretest": "npm run clean:reports",
    "merge:reports": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome.json",
    "generate:report": "marge cypress/reports/mochawesome/*.json -f report -o cypress/reports/html",
    "test:report": "npm run cy:run || true && npm run merge:reports && npm run generate:report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "cypress": "^14.4.0",
    "cypress-parallel": "^0.15.0",
    "cypress-plugin-api": "^2.11.2",
    "cypress-wait-until": "^3.0.2",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.5.0",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0"
  }
}
